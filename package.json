{
  "name": "imx-bot",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prepare": "node ./scripts/check-config.js",
    "dev": "NODE_ENV=development nodemon",
    "build": "tsc",
    "bundle": "esbuild src/index.ts --platform=node --bundle --minify --outfile=build/index.js",
    "prod": "NODE_ENV=production TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/index.js",
    "d": "npm run rebuild-start",
    "rebuild-start": "git pull && npm run build && pm2 reload ecosystem.config.js --update-env"
  },
  "dependencies": {
    "@innei/next-async": "0.3.0",
    "@mx-space/api-client": "1.3.5",
    "@types/cron": "2.0.1",
    "@types/lodash": "4.14.191",
    "@types/yargs": "17.0.23",
    "axios": "1.3.4",
    "chalk": "^4",
    "consola": "2.15.3",
    "cron": "2.3.0",
    "dayjs": "1.11.7",
    "github-webhook-handler": "1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "4.17.21",
    "oicq": "2.3.1",
    "openai": "^3.2.1",
    "randomcolor": "0.6.2",
    "remove-markdown": "0.5.0",
    "socket.io-client": "4.6.1",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@innei/eslint-config-ts": "0.9.8",
    "@innei/prettier": "0.9.8",
    "@types/node": "18.15.5",
    "@types/randomcolor": "0.5.7",
    "nodemon": "2.0.21",
    "tsconfig-paths": "4.1.2",
    "tsx": "^3.12.6",
    "typescript": "5.0.2"
  },
  "resolutions": {
    "axios": "0.27.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "oicq@2.3.1": "patches/oicq+2.3.1.patch"
    }
  }
}
