{
  "name": "imx-bot",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prepare": "node ./scripts/check-config.js",
    "dev": "NODE_ENV=development nodemon",
    "build": "tsc",
    "bundle": "esbuild src/index.ts --platform=node --bundle --minify --outfile=build/index.js",
    "prod": "NODE_ENV=production TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/index.js",
    "d": "npm run rebuild-start",
    "rebuild-start": "git pull && npm run build && pm2 reload ecosystem.config.js --update-env"
  },
  "dependencies": {
    "@innei/next-async": "0.2.3",
    "@mx-space/api-client": "0.10.14",
    "@types/cron": "2.0.0",
    "@types/lodash": "4.14.182",
    "@types/yargs": "17.0.10",
    "axios": "1.1.2",
    "camelcase-keys": "^7",
    "chalk": "^4",
    "consola": "2.15.3",
    "cron": "2.1.0",
    "github-webhook-handler": "1.0.0",
    "lodash": "4.17.21",
    "oicq": "2.3.1",
    "randomcolor": "0.6.2",
    "remove-markdown": "0.5.0",
    "socket.io-client": "4.5.2",
    "yargs": "17.6.0"
  },
  "devDependencies": {
    "@innei/eslint-config-ts": "0.9.5",
    "@innei/prettier": "0.9.5",
    "@types/node": "17.0.35",
    "@types/randomcolor": "0.5.6",
    "nodemon": "2.0.20",
    "tsconfig-paths": "4.1.0",
    "tsx": "^3.10.1",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "axios": "0.27.2"
  }
}
